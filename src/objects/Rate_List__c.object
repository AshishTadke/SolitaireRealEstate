    <validationRules>
        <fullName>SpecialDeviationOnOpportunity</fullName>
        <active>true</active>
        <description>Check the Special Deviation on Customer(Opportunity) of tagged Project Unit</description>
        <errorConditionFormula>AND(Project_Unit__r.Customers__r.Special_Deviations__c == true, ISBLANK(Project_Unit__r.Customers__r.Special_Deviation_Charge__c))</errorConditionFormula>
        <errorDisplayField>Project_Unit__c</errorDisplayField>
        <errorMessage>Please add the Special Deviation Charge or UnCheck the Special Deviation from the Customer which is Attached with this Project Unit.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Tag_Project_Unit_Only_Not_Project</fullName>
        <active>true</active>
        <description>Tagging Project Unit only on Project Stage(Pre-pre-Launch, Pre-Launch, Soft Launch)</description>
        <errorConditionFormula>AND(!ISPICKVAL(Project__r.Project_Stage__c, &apos;Launch&apos;), Project__c != null)</errorConditionFormula>
        <errorDisplayField>Project__c</errorDisplayField>
        <errorMessage>You can map a Project only on Lauch Stage (Project).</errorMessage>
    </validationRules>
