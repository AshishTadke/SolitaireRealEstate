      border-bottom-color: #777\9;
      opacity: .3;
      filter: alpha(opacity=30);      
    }

    #tabs a:hover,
    #tabs a:focus {
      border-bottom-color: #2ac7e1;
      opacity: 1;
      filter: alpha(opacity=100);
    }

    #tabs a:focus {
      outline: 0;
    }

    #tabs #current {
      z-index: 3;
      border-bottom-color: #3d3d3d;
      opacity: 2;
      filter: alpha(opacity=100);      
    }

    /* ----------- */
    #content {
        background: #D6DDDD;
        border-top: 0px solid #3d3d3d;
        padding: 2em;
        //height: 500px;
        /*background-image:"house1.jpg";*/
    }

    #content h2,
      #content h3,
