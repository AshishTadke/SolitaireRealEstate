    #resizable { margin:10px 20px 20px 170px; width: 1000px; height: 500px; padding: 0.5em; }
    #resizable h3 { text-align: center; margin: 0; }

    /* ------------------------------------------------- */

    #tabs {
      overflow: hidden;
      width: 100%;
      margin: 0;
      padding: 0;
      list-style: none;
    }

    #tabs li {
      float: left;
      margin: 0 -15px 0 0;
  }

    #tabs a {
      float: left;
      position: relative;
      padding: 0 60px;
      height: 0;
      line-height: 30px;
      text-transform: uppercase;
      text-decoration: none;
      color: #fff;      
      border-right: 30px solid transparent;
